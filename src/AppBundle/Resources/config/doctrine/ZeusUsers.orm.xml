<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="AppBundle\Entity\ZeusUsersRepository" name="AppBundle\Entity\ZeusUsers">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="username" type="string" column="username" length="15"/>
    <field name="password" type="string" column="password" length="200"/>
    <field name="email" type="string" column="email" length="100"/>
    <field name="country" type="string" column="country" length="30"/>
	<field name="notification_id" type="string" column="notification_id" length="200"/>
	<field name="action" type="string" column="action" length="20"/>
	<field name="lat" type="string" column="lat" length="20"/>
	<field name="lng" type="string" column="lng" length="20"/>
	<field name="acc" type="string" column="acc" length="10"/>
  </entity>
</doctrine-mapping>
